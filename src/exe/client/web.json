{
    "/login.do": {
        "ok": {
            "query": "login",
            "params": [ "userid", "password" ],
            "reply": "login",
            "extra": [ "token", "permissions" ],
            "template": "{{permissions}}_home",
            "cookie": {
                "write": "sessionid"
            }
        }
    },
    "/home.do": {
        "list": {
            "query": "all_list",
            "params": [ "token" ],
            "reply": "list",
            "extra": [ "token", "names" ],
            "template": "list",
            "cookie": {
                "read": "sessionid"
            }
        },
        "search": {
            "query": "tag_list",
            "params": [ "token", "tag" ],
            "reply": "list",
            "extra": [ "token", "names" ],
            "template": "list",
            "cookie": {
                "read": "sessionid"
            }
        }
    },
    "setpass.do": {
        "setrecipe": {
            "query": "set_recipe_pass",
            "params": [ "token", "key", "recipe" ],
            "reply": "pass",
            "extra": [ "token", "key", "pass", "properties" ],
            "template": "show_pass",
            "cookie": {
                "read": "sessionid"
            }
        }
    }
}
